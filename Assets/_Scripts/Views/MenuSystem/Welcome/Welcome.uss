#Footer {
    flex-grow: 1;
    width: 100%;
    height: 10%;
    justify-content: center;
    align-self: center;
    align-items: center;
    align-content: stretch;
}

#AdditionalInformation {
    color: rgb(247, 187, 39);
    -unity-font-definition: url("project://database/Assets/_Fonts/References/RobotoSerif-VariableFont_GRAD,opsz,wdth,wght.ttf?fileID=12800000&guid=107b25d6011d5425e8d420338534b760&type=3#RobotoSerif-VariableFont_GRAD,opsz,wdth,wght");
}

.LoginPanelMoveA {
    flex-grow: 1;
    position: absolute;
    width: 100%;
    height: 50%;
    bottom: 0;
    background-color: rgb(255, 255, 255);
    border-top-left-radius: 50px;
    border-top-right-radius: 50px;
    translate: 0 100%;
    transition-duration: 1s;
    transition-timing-function: ease-in-bounce;
}

.LoginPanelMoveB {
    translate: 0 0%;
}

.Scrim {
    flex-grow: 1;
    background-color: rgba(0, 0, 0, 0.78);
    opacity: 0;
    transition-duration: 0.25s;
}

.ScrimFadein {
    opacity: 1;
}

.RegisterPanelOutMainScreen {
    background-color: rgb(75, 42, 87);
    position: absolute;
    width: 100%;
    height: 50%;
    bottom: 0;
    translate: 100% 0;
    transition-timing-function: ease-in-bounce;
    transition-duration: 1s;
}

.RegisterPanelInMainScreen {
    translate: 0% 0;
}

.RecoverPasswordPanelOutMainScreen {
    background-color: rgb(75, 42, 87);
    position: absolute;
    width: 100%;
    height: 50%;
    bottom: 0;
    translate: -100% 0;
    right: auto;
    transition-timing-function: ease-in-bounce;
    transition-duration: 1s;
}

.RecoverPasswordPanelInMainScreen {
    translate: 0% 0;
}
